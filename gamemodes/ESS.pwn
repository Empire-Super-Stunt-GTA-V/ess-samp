function UpdateClan(varname[], amount, id)
{
	eQuery[0] = EOS;
	//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
	mysql_format(SQL, eQuery, sizeof(eQuery), "UPDATE `Clans` SET `%s` = '%d' WHERE `ID` = '%d'", varname, amount, id);
	mysql_tquery(SQL, eQuery, "", "");
	return true;
}
