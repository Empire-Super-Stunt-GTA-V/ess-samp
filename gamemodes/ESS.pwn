	}
	//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    // -> Steal The Car
    //++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    if(PlayerInfo[playerid][InStealCar] == 1)
	{
		if(ServerInfo[StealCarOwner] == playerid)
	   	{
        	for(new i; i < MAX_PLAYERS; i++)
            {
                if(PlayerInfo[i][InStealCar] == 1)
			 	{
	                PlayerInfo[playerid][InStealCar] = 0;
	                //++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
