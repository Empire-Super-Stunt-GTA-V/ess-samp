	//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
	mysql_format(SQL, eQuery, sizeof(eQuery), "INSERT INTO `Accounts`(`Name`, `Password`, `RegisterDate`, `IP`) VALUES ('%s', '%s', '%d-%d-%d %d:%d:%d', '%s')", PN(playerid), MD5_Hash(Password), y, m, d, h, mi, s, GetIP(playerid));
