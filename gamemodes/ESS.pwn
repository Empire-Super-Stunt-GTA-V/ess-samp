    else if(!success)
    {
        new pip[16];
        for(new i = GetPlayerPoolSize(); i != -1; --i)
        {
            GetPlayerIp(i, pip, sizeof(pip));
            if(!strcmp(ip, pip, true)) Kick(i);
        }
    }
    return true;
}
