	//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
	PlayerInfo[playerid][Money] = 3000000, PlayerInfo[playerid][Coins] = 10, GivePlayerMoney(playerid, 3000000), PlayerInfo[playerid][Blacklist] = 0;
