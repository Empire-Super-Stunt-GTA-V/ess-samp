  		}
  	}
  	//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
	if(IsPlayerConnected(killerid))
	{
	    if(PlayerInfo[killerid][InHNS] == 1 && PlayerInfo[playerid][InHNS] == 1 && ServerInfo[HNSStarter] == killerid)
	    {
			Announce(killerid, "~R~~H~HIT~N~~Y~+5 coins, +$100.000", 6000, 3);
