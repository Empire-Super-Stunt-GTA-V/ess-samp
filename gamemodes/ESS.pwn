			if(SumoCar[i] != 0) DestroyVehicle(SumoCar[i]), SumoCar[i] = 0;
			//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
			PlayerInfo[i][SumoSeat] = 0;
			//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
