stock CalculateClanPoints(playerid)
{
	PlayerInfo[playerid][c_Total] = (PlayerInfo[playerid][c_Points] + PlayerInfo[playerid][c_Kills] + PlayerInfo[playerid][c_Captures]);
	return PlayerInfo[playerid][c_Total];
}
