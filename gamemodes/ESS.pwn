				//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    		}
 		}
	}
	//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
	SetPlayerVirtualWorld(playerid, rID+6969), SetVehicleVirtualWorld(GetPlayerVehicleID(playerid), GetPlayerVirtualWorld(playerid));
