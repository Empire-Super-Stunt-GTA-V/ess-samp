function DisableCollisionsForSumo()
{
    foreach(new i : Player)
	{
		if(PlayerInfo[i][InSumo] == 1)
		{
		    DisableRemoteVehicleCollisions(i, 0);
		    Announce(i, "~R~~H~The collisions has~N~~R~~H~been disabled!", 6000, 3);
