stock GetClanID(playerid)
{
	for(new i = 1; i < MAX_CLANS; i++) if(IsPlayerInRangeOfPoint(playerid, 3.0, Clan[i][ClanPos][0], Clan[i][ClanPos][1], Clan[i][ClanPos][2])) return i;
