             PlayerInfo[DuelPlayerInvite][InDuel] = 0;
             mysql_format(SQL, eQuery, sizeof(eQuery), "DELETE FROM `Duels` WHERE `Name` = '%s'", PN(DuelPlayerInvite));
             mysql_tquery(SQL, eQuery, "", "");
		}
        //++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
     	SpawnPlayer(playerid);
	}
	return true;
